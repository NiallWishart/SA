        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentation"
        mc:Ignorable="d"
        Title="Finance" Height="400.000" Width="500.000">
    <Grid>
        <ListBox x:Name="ListFinances" HorizontalAlignment="Left" Height="291" VerticalAlignment="Top" Width="281" Margin="110,10,0,0"/>
        <Button x:Name="btnBack" Content="&lt; Back" HorizontalAlignment="Left" Height="18" Margin="10,341,0,0" VerticalAlignment="Top" Width="51" Click="btnBack_Click" RenderTransformOrigin="0.937,0.462"/>
        <Button x:Name="btnApprove" Content="Approve" HorizontalAlignment="Left" Height="56" Margin="10,174,0,0" VerticalAlignment="Top" Width="95" Click="btnApprove_Click"/>
        <Button x:Name="btnRemove" Content="Deny" HorizontalAlignment="Left" Height="56" Margin="396,174,0,0" VerticalAlignment="Top" Width="86" Click="btnRemove_Click"/>
        <Button x:Name="btnRefresh" Content="Refresh Requests" HorizontalAlignment="Left" Height="53" Margin="197,306,0,0" VerticalAlignment="Top" Width="107" Click="btnRefresh_Click" RenderTransformOrigin="0.327,0.496"/>

    </Grid>
</Window>
